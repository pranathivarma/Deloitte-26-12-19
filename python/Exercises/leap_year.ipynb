     "text": [
      "Enter a year: 2020\n",
      "It's a leap year\n"
     ]
    }
   ],
   "source": [
    "year = int(input(\"Enter a year: \"))\n",
    "if year%4!=0:\n",
    "    print(\"It's not a leap year\")\n",
    "else :\n",
    "    if year%100 == 0:\n",
    "        if year%400 == 0:\n",
    "            print(\"It's a leap year\")\n",
    "        else :\n",
    "            print(\"It's not a leap year\")\n",
    "    else :\n",
    "        print(\"It's a leap year\")"
   ]
